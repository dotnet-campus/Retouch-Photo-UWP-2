<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    >

    <!--Separator Row-->
    <DataTemplate x:Name="AppRowSeparator">
        <Rectangle Height="1" StrokeDashArray="2 2" Stroke="{ThemeResource AppBorderBrush}" />
    </DataTemplate>
    <!--Separator Column-->
    <DataTemplate x:Name="AppColumnSeparator">
        <Rectangle Width="1" StrokeDashArray="2 2" Stroke="{ThemeResource AppBorderBrush}" />
    </DataTemplate>
    
</ResourceDictionary>
