<UserControl
    x:Class="Retouch_Photo.Controls.AppbarControls.AppbarPicturesControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:conAppbarControls="using:Retouch_Photo.Controls.AppbarControls"
    mc:Ignorable="d"
    d:DesignHeight="50"
    d:DesignWidth="800">
    

    <StackPanel Orientation="Horizontal">

        <conAppbarControls:AppBarControlButton x:Name="PhotoButton" Glyph="&#xEB9F;"  Text="Library" Tap="PhotoButton_Tapped"/>
        <conAppbarControls:AppBarControlButton x:Name="DestopButton"  Glyph="&#xEC4E;"  Text="Destop" Tap="DestopButton_Tapped"/>
     
        <AppBarSeparator/>

        <conAppbarControls:AppBarControlButton x:Name="CancelButton" Glyph="&#xE106;"  Text="Cancel" Tap="CancelButton_Tapped"/>

    </StackPanel>
    
</UserControl>
